owOption = 1		ifTrue: [ self renderAboutOn: html ].	showOption = 2		ifTrue: [ html render: (RespuestaForm preg: preg usuario: usuarioLogueado ) ].	showOption = 3		ifTrue: [ html render: (UsuarioView usuario: usuarioLogueado obj: self) ].	showOption = 4		ifTrue: [ html 	render: (PreguntaForm usuario: usuarioLogueado topic: CuOOraApp obtenerTodosLosTopicos) ].	"showOption = 5		ifTrue: [preg obtenerRespuestas do: [ :r | html render: (RespuestaView new resp: r) ] ]."	 showOption = 0 		ifFalse: [ ^ self ].	html heading		level: 3;		with: 'Preguntas relevantes para: ' , usuarioLogueado nombreApe.	usuarioLogueado obtenerPreguntasRelevantes isNotEmpty		ifTrue: [ usuarioLogueado obtenerPreguntasRelevantes				do: [ :p | html render: (PreguntaView preg: p obj: self) ] ]		ifFalse: [ html text: 'El usuario no tiene preguntas relevantes' ]! !!CuOOraWeb methodsFor: 'rendering' stamp: 'Vic 9/1/2019 11:38'!renderBaseOn: html	usuarioLogueado isNil		ifTrue: [ self renderLoginOn: html ]		ifFalse: [ self renderHomeOn: html ].	err = 1		ifTrue: [ self renderErrorOn: html error: ' Nombre de usuario o contraseña incorrectos.' ].	! !!CuOOraWeb methodsFor: 'rendering' stamp: 'Vic 11/17/2019 22:58'!renderLoginOn: html	html div		id: 'div-login';		with: [ html 				tbsForm: [ html tbsPanel beDefault						with: [ html heading								level: 3;								with: 'Ingreso al sistema'.							html								tbsPanelBody: [ html										tbsFormGroup: [ html label												for: 'txtUser';												with: 'Usuario'.											html textInput required												tbsFormControl;												id: 'txtUser';												"value: usuario;"													callback: [ :usr | self usuario: usr ];												placeholder: 'Ingrese nombre' ].									html										tbsFormGroup: [ html label												for: 'txtPassword';												with: 'Clave'.											html passwordInput required												tbsFormControl;												id: 'txtPassword';												callback: [ :pass | sel